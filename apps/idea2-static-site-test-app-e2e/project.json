{
  "name": "idea2-static-site-test-app-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/idea2-static-site-test-app-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": [
        "{workspaceRoot}/dist/.playwright/apps/idea2-static-site-test-app-e2e"
      ],
      "options": {
        "config": "apps/idea2-static-site-test-app-e2e/playwright.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/idea2-static-site-test-app-e2e/**/*.{js,ts}"]
      }
    }
  },
  "implicitDependencies": ["idea2-static-site-test-app"]
}
